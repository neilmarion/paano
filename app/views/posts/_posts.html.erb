<% posts.each do |post| %>
  <tr class = "post-row">
    <td>
      <%= render 'posts/post/reputations', post: post %>
      <%= render 'posts/post/title', post: post %> 
      <%= render 'posts/post/preview', post: post %> 
      <%= render 'posts/post/other', post: post %> 
      <%= render 'posts/post/controls' %>
    </td>
  </tr>
<% end %>
